### Addresses
# Get all addresses
GET http://localhost:8080/addresses
Accept: application/json

# Get address by ID
GET http://localhost:8080/addresses/11111111-1111-1111-1111-111111111111
Accept: application/json

# Create address
POST http://localhost:8080/addresses
Content-Type: application/json
{
  "type": "property",
  "street": "Calle Nueva",
  "number": "321",
  "neighborhood": "Centro",
  "city": "Metrópolis",
  "state": "CA",
  "zipCode": "90004",
  "country": "México"
}

# Update address
PUT http://localhost:8080/addresses/11111111-1111-1111-1111-111111111111
Content-Type: application/json
{
  "street": "Calle Actualizada"
}

# Delete address
DELETE http://localhost:8080/addresses/11111111-1111-1111-1111-111111111111

### Users
# Get all users
GET http://localhost:8080/users
Accept: application/json

# Get user by ID
GET http://localhost:8080/users/aaaaaaaa-aaaa-aaaa-aaaa-aaaaaaaaaaaa
Accept: application/json

# Create user
POST http://localhost:8080/users
Content-Type: application/json
{
  "type": "tenant",
  "addressId": "22222222-2222-2222-2222-222222222222",
  "firstName": "Juan",
  "middleName": "P.",
  "lastName": "Pérez",
  "email": "juan.perez@example.com",
  "phone": "+525511223347"
}

# Update user
PUT http://localhost:8080/users/aaaaaaaa-aaaa-aaaa-aaaa-aaaaaaaaaaaa
Content-Type: application/json
{
  "lastName": "Actualizado"
}

# Delete user
DELETE http://localhost:8080/users/aaaaaaaa-aaaa-aaaa-aaaa-aaaaaaaaaaaa

### Contracts
# Get all contracts
GET http://localhost:8080/contracts
Accept: application/json

# Get contract by ID
GET http://localhost:8080/contracts/dddddddd-dddd-dddd-dddd-dddddddddddd
Accept: application/json

# Create contract
POST http://localhost:8080/contracts
Content-Type: application/json
{
  "tenantId": "bbbbbbbb-bbbb-bbbb-bbbb-bbbbbbbbbbbb",
  "addressId": "11111111-1111-1111-1111-111111111111"
}

# Update contract
PUT http://localhost:8080/contracts/dddddddd-dddd-dddd-dddd-dddddddddddd
Content-Type: application/json
{
  "addressId": "22222222-2222-2222-2222-222222222222"
}

# Delete contract
DELETE http://localhost:8080/contracts/dddddddd-dddd-dddd-dddd-dddddddddddd

### Contract Versions
# Get all contract versions
GET http://localhost:8080/contract-versions
Accept: application/json

# Get contract version by ID
GET http://localhost:8080/contract-versions/eeeeeeee-eeee-eeee-eeee-eeeeeeeeeeee
Accept: application/json

# Create contract version
POST http://localhost:8080/contract-versions
Content-Type: application/json
{
  "contractId": "dddddddd-dddd-dddd-dddd-dddddddddddd",
  "versionNumber": 2,
  "deposit": 1200,
  "rent": 1600,
  "rentIncreasePercentage": 6,
  "business": "Negocio B",
  "status": "active",
  "type": "yearly",
  "startDate": "2026-01-01",
  "endDate": "2026-12-31"
}

# Update contract version
PUT http://localhost:8080/contract-versions/eeeeeeee-eeee-eeee-eeee-eeeeeeeeeeee
Content-Type: application/json
{
  "rent": 1700
}

# Delete contract version
DELETE http://localhost:8080/contract-versions/eeeeeeee-eeee-eeee-eeee-eeeeeeeeeeee

### Contract References
# Get all contract references
GET http://localhost:8080/contract-references
Accept: application/json

# Get contract reference by contract and reference ID
GET http://localhost:8080/contract-references?contractId=dddddddd-dddd-dddd-dddd-dddddddddddd&referenceId=cccccccc-cccc-cccc-cccc-cccccccccccc
Accept: application/json

# Create contract reference
POST http://localhost:8080/contract-references
Content-Type: application/json
{
  "contractId": "dddddddd-dddd-dddd-dddd-dddddddddddd",
  "referenceId": "cccccccc-cccc-cccc-cccc-cccccccccccc"
}

# Delete contract reference
DELETE http://localhost:8080/contract-references?contractId=dddddddd-dddd-dddd-dddd-dddddddddddd&referenceId=cccccccc-cccc-cccc-cccc-cccccccccccc

